    ctrl nop
    ctrl addi R2 ZERO 255
    ctrl addi R1 ZERO 150
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1
    ctrl sll R1 R1

    ctrl addi VADDR ZERO 0

pixel_loop:

    ctrl lw VDATA VADDR
    ctrl nop
    ctrl sub VDATA R2 VDATA
    ctrl nop
    ctrl addi VADDR VADDR 1

    ctrl beq infinite_loop
    ctrl slt ZERO R1 VADDR
    ctrl nop

    ctrl jump pixel_loop

infinite_loop:

    ctrl nop
    ctrl jump infinite_loop
